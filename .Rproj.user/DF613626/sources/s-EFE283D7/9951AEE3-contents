#list 
#점(스칼라) a return(.....)
#-선-면-큐브
#Vector is atomic vector
#List is recursive vector

x<-c("홍길동",175.6,5000,T)
x
lst<-list("홍길동",175.6,5000,T)
lst

x[1] #벡터는 중괄호 하나
lst[[1]] #리스트는 중괄호 두번 (벡터의 벡터이므로)
lst<-list(name="홍길동",height=176.6,sal=5000,employee=T)
#name, height 등은 attribute, metadata이다.속성값은 ""이 없음.
lst$sal
lst$name

length(lst)
